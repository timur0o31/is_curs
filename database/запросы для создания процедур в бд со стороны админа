curl -i -X POST http://localhost:8080/api/procedures \
  -H "Authorization: Bearer $ADMIN" \
  -H "Content-Type: application/json" \
  -d '{"name":"Массаж","description":"Локальный массаж спины и шейно-воротниковой зоны","defaultSeats":1,"duration":"PT30M"}'

